@using ImageContestSystem.Web.Models.ViewModels
@model IEnumerable<PhotoViewModel>

<div class="jumbotron">
    <h1>Image contest system</h1>
</div>

<div class="container">
    <div class="row">
        <div class="col-md-3">
            <div>
                @Html.Partial("_UserAsideMenu")
            </div>
        </div>

        <div class="col-md-9" id="main-content">
            <div class="row">
                @foreach (var photo in Model)
                {
                    @Html.DisplayFor(p => photo)
                }
            </div>
        </div>
    </div>
</div>